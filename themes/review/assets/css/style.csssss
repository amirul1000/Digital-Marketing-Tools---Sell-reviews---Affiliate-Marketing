  }
  nav.navbar #navbar ul.navbar-nav li,
  nav.navbar #navbar ul.navbar-nav a {
    color: #fff !important;
    padding: 6px 0 26px;
    margin-right: 2px;
  }

  nav.navbar #navbar ul.navbar-nav a {
    color: #000;
    text-transform: uppercase;
    /* border: 1px solid transparent; */
    font-weight: 700;
    font-size: 14px;

    padding: 10px 8px;
  }
  nav.navbar #navbar ul.navbar-nav a:not(:last-child) {
    margin-right: 2px;
  }

  #res {
    color: #fff;
    text-transform: uppercase;
    /* border: 1px solid transparent; */
    font-weight: 700;
    font-size: 13px;
    padding: 15px 8px;

    border-radius: 20%;
  }
  nav.navbar #navbar ul.navbar-nav a.active {
    border: 2px solid #fff;
    border-radius: 20%;
  }
  /* ul li.nav-active a {
    color: #fff !important;
  } */
  /* .active {
    background: transparent;
    color: #fff !important;
    background-color: #000;
    border-radius: 10px;
  } */
  /* ul li.nav-active a, */

  nav.navbar #navbar ul.navbar-nav a:hover {
    background: transparent;
    background-color: transparent;
    border: 2px solid #fff;
    border-radius: 20%;
  }

  /* nav.navbar #navbar ul.navbar-nav a:hover:before {
    visibility: visible;
    opacity: 1;
    transition-delay: 0.1s;
    left: 50%;
  } */
  nav.navbar #navbar ul.navbar-nav a:focus {
    background: transparent;
  }
  nav.navbar #navbar ul.navbar-nav > li > a:before {
    zoom: 1;
    filter: alpha(opacity=0);
    -webkit-opacity: 0;
    -moz-opacity: 0;
    opacity: 0;
    visibility: hidden;
    transition: all 0.3s ease;
    content: "";
    position: absolute;
    width: 26px;
